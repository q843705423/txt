set clipboard+=unnamed

set keep-english-in-normal
set keep-english-in-normal-and-restore-in-insert


nnoremap <Space>sc :nohlsearch<CR>

" Redo
nnoremap U <C-r>

" Insert mode shortcut
inoremap <C-h> <Left>
inoremap <C-j> <Down>
inoremap <C-k> <Up>
inoremap <C-l> <Right>
inoremap <C-a> <Home>
inoremap <C-e> <End>
inoremap <C-d> <Delete>

nnoremap gr :action RenameElement<CR>
nnoremap gf :action FindInPath<CR>
nnoremap gp :action ShowPopupMenu<CR>
nnoremap gs :action SurroundWith<CR>
vnoremap gs :action SurroundWith<CR>
nnoremap ge :action GotoNextError<CR>
nnoremap gE :action GotoPreviousError<CR>
nnoremap g= :action ReformatCode<CR>
nnoremap g{ :action EditorCodeBlockStart<CR>
nnoremap g} :action EditorCodeBlockEnd<CR>
nnoremap ga :action Generate<CR>
nnoremap gw :action Generate<CR>
"go back
nnoremap gb :action Back<CR>
"go back re
nnoremap gB :action Forward<CR>
vnoremap gw :action Generate<CR>
"it run
nnoremap zr :action Run<CR>
"it run
nnoremap zR :action RunClass<CR>
"it  debug
nnoremap zd :action DebugClass<CR>

"it stop
nnoremap zs  :action Stop<CR>

"zap breakPoint
nnoremap zb :action ToggleLineBreakpoint<CR>

"zap here
nnoremap zh :action ForceRunToCursor<CR>

"zap find
nnoremap zF :action ShowFilePath<CR>

"z method extract
nnoremap zm :action ExtractMethod<CR>
vnoremap zm :action ExtractMethod<CR>

          
"zap pop fileStructure
nnoremap zf :action FileStructurePopup<CR>

"ze
nnoremap ze :action RecentFiles<CR>
nnoremap zE :action RecentChangedFiles<CR>

"zv
nnoremap zv :action IntroduceVariable<CR>
